{
  "name": "@jfront/ui-textarea",
  "version": "0.1.1",
  "license": "Apache-2.0",
  "main": "../textarea/dist/cjs/index.js",
  "module": "../textarea/dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "typings": "dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "../textarea/dist"
  ],
  "scripts": {
    "prebuild": "rimraf ../textarea/dist",
    "build": "concurrently yarn:build:*",
    "build:esm": "cross-env BABEL_ENV=esm babel ../textarea/src --root-mode upward --extensions .ts,.tsx -d dist/esm --source-maps --copy-files",
    "build:cjs": "cross-env BABEL_ENV=cjs babel ../textarea/src --root-mode upward --extensions .ts,.tsx -d dist/cjs --source-maps --copy-files",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir ../textarea/dist/types",
    "test": "jest --passWithNoTests",
    "test:cov": "yarn test --coverage"
  },
  "dependencies": {
    "@jfront/ui-hooks": "^0.3.3",
    "@jfront/ui-icons": "^0.2.1",
    "react-number-format": "4.4.1",
    "react-text-mask": "5.4.3"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  },
  "devDependencies": {
    "@jfront/ui-label": "^0.1.0",
    "@types/node": "*",
    "@types/react-text-mask": "5.4.6",
    "@types/styled-components": "^5.0.1"
  }
}
